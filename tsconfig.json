{
  "compilerOptions": {
    "module": "esnext",
    "target": "es6",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": "src",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ]
  },
  "include": [
    "src/ts/*"
, "src/ts/views/Login.tsx", "src/ts/views/SignUp1.tsx", "src/ts/utility/serviceWorker.ts", "src/ts/utility/setupTests.ts", "src/ts/utility/Search.ts", "src/ts/utility/App.test.js"  ]
}
